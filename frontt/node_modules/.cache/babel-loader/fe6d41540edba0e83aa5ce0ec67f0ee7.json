["name","data","teachers","currentTeacher","currentIndex","methods","retrieveTeachers","TeacherDataService","then","console","refreshList","removeAllTeachers","setActiveTeacher","teacher","index","mounted"],"sourceRoot":"src/components","sources":["TeacherListt.vue"],"sourcesContent":["<template>\n  <div class=\"list row\">\n    <div class=\"col-md-6\">\n      <h4>Teachers List</h4>\n      <h3 v-if=\"teachers.length==0\">ddasd</h3>\n      <ul class=\"list-group\">\n        <li\n          class=\"list-group-item\"\n          :class=\"{ active: index == currentIndex }\"\n          v-for=\"(teacher, index) in teachers\"\n          :key=\"index\"\n          @click=\"setActiveTeacher(teacher, index)\"\n        >\n          {{ teacher.name }}\n        </li>\n      </ul>\n      <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllTeachers\">\n        Remove All\n      </button>\n    </div>\n    <div class=\"col-md-6\">\n      <div class=\"col-md-6\">\n        <div v-if=\"currentTeacher\">\n          <h4>Teacher</h4>\n          <div>\n            <label><strong>Name:</strong></label> {{ currentTeacher.name }}\n          </div>\n          <div>\n            <label><strong>Description:</strong></label>\n            {{ currentTeacher.designation }}\n          </div>\n          <div>\n            <label><strong>Status:</strong></label>\n            {{ currentTeacher.presence ? \"Present\" : \"Absent\" }}\n          </div>\n\n          <router-link\n            :to=\"'/teachers/' + currentTeacher.id\"\n            class=\"badge badge-warning\"\n            >Edit</router-link\n          >\n        </div>\n        <div v-else>\n          <br />\n          <p>Please click on a Teacher...</p>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n<script>\nimport TeacherDataService from \"@/services/TeacherDataService\";\nexport default {\n  name: \"TeacherListt\",\n  data() {\n    return {\n      teachers: [],\n      currentTeacher: null,\n      currentIndex: -1,\n    };\n  },\n  methods: {\n    retrieveTeachers() {\n      TeacherDataService.getAll()\n        .then((response) => {\n          this.teachers = response.data;\n          console.log(response.data);\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n    refreshList() {\n      this.retrieveTeachers();\n      this.currentTeacher = null;\n      this.currentIndex = -1;\n    },\n    removeAllTeachers() {\n      TeacherDataService.deleteAll()\n        .then((response) => {\n          console.log(response.data);\n          this.refreshList();\n        })\n        .catch((e) => {\n          console.log(e);\n        });\n    },\n    setActiveTeacher(teacher, index) {\n      this.currentTeacher = teacher;\n      this.currentIndex = teacher ? index : -1;\n    },\n  },\n  mounted() {\n    this.retrieveTeachers();\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module"}